FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "30";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 9e-006;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 9e-006;
    }
    outlet
    {
        type            calculated;
        value           uniform 9e-006;
    }
    boundaries
    {
        type            calculated;
        value           uniform 9e-006;
    }
    Starship_Puck_V2_v6
    {
        type            nutUWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0;
    }
}


